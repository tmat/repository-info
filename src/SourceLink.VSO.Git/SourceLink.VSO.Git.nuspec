<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>$PackageId$</id>
    <description>$Description$</description>
    <version>$Version$</version>
    <authors>$Authors$</authors>
    <requireLicenseAcceptance>$RequireLicenseAcceptance$</requireLicenseAcceptance>
    <licenseUrl>$PackageLicenseUrl$</licenseUrl>
    <projectUrl>$PackageProjectUrl$</projectUrl>
    <copyright>$Copyright$</copyright>
    <developmentDependency>$DevelopmentDependency$</developmentDependency>
    <tags>$PackageTags$</tags>
    <serviceable>$Serviceable$</serviceable>
    <repository type="$repositoryType$" url="$repositoryUrl$" commit="$repositoryCommit$" />
    <dependencies>
      <dependency id="Microsoft.Build.Tasks.Git" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="netcoreapp1.0\*.*" target="tools\netcoreapp1.0" />
    <file src="net46\SourceLink.*" target="tools\net46" />

    <file src="$ProjectDirectory$\build\*.*" target="build" />
  </files>
</package>